{
  "expo": {
    "name": "flametune",
    "slug": "flametune",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "flametune",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.frerot.flametune",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.frerot.flametune",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "assetBundlePatterns": ["**/*"],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/your-update-url"
    },
    "notification": {
      "icon": "./assets/images/logo.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true
    },
    "owner": "frerot",
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
